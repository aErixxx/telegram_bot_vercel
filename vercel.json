{
  "version": 2,
  "functions": {
    "api/*.py": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "api/webhook.py"
    },
    {
      "src": "/webhook",
      "methods": ["POST"],
      "dest": "api/webhook.py"
    },
    {
      "src": "/status",
      "methods": ["GET"],
      "dest": "api/webhook.py"
    }
  ]
}
