{
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@4.3.1"
    }
  },
  "routes": [
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "api/webhook.py"
    },
    {
      "src": "/webhook",
      "methods": ["POST"],
      "dest": "api/webhook.py"
    },
    {
      "src": "/health",
      "methods": ["GET"],
      "dest": "api/webhook.py"
    }
  ]
}
